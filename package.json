{
  "name": "fascia",
  "version": "2.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android --appIdSuffix dev",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "typecheck": "tsc",
    "typecheck:watch": "tsc --watch",
    "build:android:staging": "cd android && ./gradlew assembleStaging",
    "build:android:release": "cd android && ./gradlew bundleRelease"
  },
  "dependencies": {
    "@expo/react-native-action-sheet": "^3.8.0",
    "@react-native-async-storage/async-storage": "^1.17.7",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-native-picker/picker": "^2.4.2",
    "@react-navigation/drawer": "^5.9.0",
    "@react-navigation/native": "^5.7.3",
    "@react-navigation/stack": "^5.9.0",
    "axios": "^0.27.2",
    "react": "^17.0.2",
    "react-hook-form": "^6.15.8",
    "react-native": "0.65.2",
    "react-native-action-button": "^2.8.5",
    "react-native-color-picker": "^0.6.0",
    "react-native-dropdownalert": "^4.3.0",
    "react-native-dynamic": "^1.0.0",
    "react-native-gesture-handler": "^1.7.0",
    "react-native-markdown-renderer": "^3.2.8",
    "react-native-modal": "^12.1.0",
    "react-native-picker-select": "^8.0.0",
    "react-native-reanimated": "^1.13.0",
    "react-native-safe-area-context": "^3.1.6",
    "react-native-screens": "^3.13.1",
    "react-native-vector-icons": "^9.2.0",
    "react-native-webview": "^11.22.3",
    "react-redux": "^8.0.2",
    "redux": "^4.2.0",
    "redux-thunk": "^2.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@babel/preset-env": "^7.1.6",
    "@babel/runtime": "^7.8.4",
    "@react-native-community/eslint-config": "^1.1.0",
    "@types/jest": "^28.1.3",
    "@types/react-dom": "^18.0.5",
    "@types/react-native": "^0.63.2",
    "@types/react-native-vector-icons": "^6.4.10",
    "@types/react-redux": "^7.1.9",
    "@types/react-test-renderer": "^16.9.2",
    "@types/redux-logger": "^3.0.9",
    "@types/redux-thunk": "^2.1.0",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "babel-jest": "^28.1.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^6.5.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^28.1.1",
    "metro-react-native-babel-preset": "^0.66.0",
    "prettier": "^2.7.1",
    "react-dom": "^17.0.2",
    "react-native-codegen": "^0.0.7",
    "react-test-renderer": "^17.0.2",
    "redux-logger": "^3.0.6",
    "typescript": "^4.7.4"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "setupFiles": [
      "./jest/supports/async-storage.js",
      "./jest/supports/react-navigation.js"
    ],
    "moduleNameMapper": {
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "identity-obj-proxy"
    }
  }
}
